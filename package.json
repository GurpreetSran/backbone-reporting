    "bower": "^1.4.1",
    "http-server": "^0.8.0",
    "httpserver": "^0.2.0",
    "jasmine-core": "^2.3.4",
    "json-server": "^0.7.20",
    "karma": "^0.12.37",
    "karma-chrome-launcher": "^0.2.0",
    "karma-jasmine": "^0.3.6",
    "karma-sinon": "^1.0.4",
    "karma-spec-reporter": "0.0.19",
    "sinon": "^1.15.4"
  }
}
